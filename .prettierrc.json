{
  "$schema": "http://json.schemastore.org/prettierrc",
  "trailingComma": "es5",
  "tabWidth": 4,
  "semi": true,
  "printWidth": 300,
  "singleQuote": true,
  "overrides": [{
    "files": "*.vue",
    "options": {
      "bracketSameLine": true,
      "vueIndentScriptAndStyle": true,
      "printWidth": 9000
    }
  }]
}
